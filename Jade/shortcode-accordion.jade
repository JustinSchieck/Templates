extends module/form
block page-config
    -page_option.body_class = null;
    -page_option.count_to = false;
    -page_option.count_down = false;
    -page_option.slider = false;
    -page_option.isotope = false;
    -page_option.slip_screen = false;
    -page_option.bootstrap_js = true;
    -page_option.jquery_ui = false;
    -page_option.magnific_popup = false;
block page-data
block before-main
    +main-nav-2-b1
block header
    +common-static-header('ACCORDION')
block main
    section.no-padding-bottom
        .container
            .row
                .col-xs-12.text-center
                    h4.bgc-gray(style="padding: 20px; margin-bottom: 50px") STYLE 1
                .col-sm-6.col-xs-12(style="margin-bottom: 30px")
                    +accordion-01
                .col-sm-6.col-xs-12(style="margin-bottom: 30px")
                    +accordion-02
    section.one-child
        .container
            .row
                .col-xs-12.text-center
                    h4.bgc-gray(style="padding: 20px; margin-bottom: 50px") STYLE 2
                .col-sm-6.col-xs-12(style="margin-bottom: 30px")
                    +accordion-03
                .col-sm-6.col-xs-12(style="margin-bottom: 30px")
                    +accordion-04

block footer
    +footer-preset-02
block after-main