extends module/form
block page-config
    -page_option.body_class = null;
    -page_option.count_to = false;
    -page_option.count_down = false;
    -page_option.slider = false;
    -page_option.isotope = false;
    -page_option.slip_screen = false;
    -page_option.bootstrap_js = true;
    -page_option.jquery_ui = false;
    -page_option.magnific_popup = false;
block page-data
block before-main
    +main-nav-2-b1
block header
    +common-static-header('TABS')
block main
    section.no-padding-bottom
        .container
            +tab-section-01("style-1","tab-1")
    section.one-child
        .container
            .row
                .col-sm-6.col-xs-12
                    +tab-section-01("style-2","tab-2")
                .col-sm-6.col-xs-12
                    +tab-section-01("style-3","tab-3")

block footer
    +footer-preset-02
block after-main