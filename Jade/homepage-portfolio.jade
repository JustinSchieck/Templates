block main
    section.no-padding.bgc-gray
        +portfolio-special-index-homepage-portfolio-filter
    section
        +section-header-1-1(data_portfolio_header)
        +portfolio-special-index-homepage-portfolio
    section.no-padding
        +call-to-action("style-2","border-btn border-btn-main btn-size-5")
    section.one-child
        +brand-carousel-1
