extends module/form
block page-config
    -page_option.body_class = null;
    -page_option.count_to = true;
    -page_option.count_down = false;
    -page_option.slider = true;
    -page_option.isotope = false;
    -page_option.slip_screen = false;
    -page_option.bootstrap_js = true;
    -page_option.jquery_ui = false;
    -page_option.magnific_popup = false;
block page-data
    -var data_team_header ={title : "MEET OUR TEAM" , caption :"Most businesses think that product is the most important thing, but without great leadership, mission and a team that deliver results at a high level, even the best product won't make a company successful."}
    -var data_service_header = { title: "OUR SERVICES" , caption:"Wolverine Creative are an independent , one-stop creative specialist  with the experience & the know how to come up with great business solutions and make them happen"}
    -var data_pricing_table_header = { title: "CHECK OUR PRICES" , caption:"Design is our art and our passion. Our goal is to create the best products with a pixel-perfect eye for detail and a high standard for aesthetic excellence. Let us help you make your website simple, beautiful and professional."}
block before-main
    +main-nav-2-b1
block header
    header.site-header
    +common-static-header('ABOUT US')
block main
    section.no-padding
        +page-about
    section.large-space
        +section-header-2-2(data_service_header)
        +service-section-02
    section.bgc-gray
        +counto-section
    section
        +section-header-2-1(data_team_header)
        +team-section-01
    section.large-space
        .parallax-bg.quote-slider-parallax(data-stellar-background-ratio="0.25" style="background-image: url('assets/images/quote-bg.jpg');")
        +quote-slider-02('color-light')
    section.one-child
        +section-header-2-2(data_pricing_table_header)
        +section-price-table
    section.one-child(style="background: #444")
        +brand-carousel-2
block footer
    +footer-preset-02
block after-main
           