                            h1 We are currently working on something awesome. Stay tuned!
                            form
                                input(type="email" placeholder="Email Address")
                                a(href="#").normal-btn.normal-btn-main.btn-size-5 SUBCRIBLE
                            +social-default-01
//- ************** Page Login ************** -//
mixin page-login(Class,ID)
    .page-login-wrapper
        .page-login(class=Class id=ID)
            form
                h3 WELCOME BACK!
                p Login to your account below
                .infor
                    span.icon
                        i.icon-user
                    input(type="email" placeholder="Email Address")
                .infor
                    span.icon
                        i.icon-key
                    input(type="password" placeholder="PassWord")
                .remember
                    label.checkbox
                        input(type="checkbox")
                        span
                    p Remember me
                .button
                    a(href="#").normal-btn.normal-btn-main.btn-size-5 LOGIN
                .button
                    a(href="#").border-btn.border-btn-lightgray.btn-size-5 SIGN UP
                a.forgotpass(href="#") Forgotten password?
            +back-to-home

//- ************** Page Regsiter ************** -//
mixin regsiter(Class,ID)
    .page-rigsiter-wrapper
        .page-rigsiter(class=Class id=ID)
            form
                h3  REGSITER NOW!
                .name
                    p NAME
                    input(type="text" name="first_name" placeholder="First")
                    input(type="text" name="last_name" placeholder="Last")
                p ENTER YOUR EMAIL ADDRESS:
                input(type="email" placeholder="Address your email.")
                p CREATE YOUR USERNAME:
                input(type="text" name="username")
                p CREATE YOUR PASSWORD:
                input(type="password" name="password")
                p CONFIRM YOUR PASSWORD:
                input(type="password" name="confirm_password")
                span By clicking “Create Account” you agree to the terms and conditions of the following:
                    a(href="#") Universal Terms of Service
                    |&nbsp;&nbsp;
                    a(href="#") Privacy Policy
                .button
                    a(href="#").normal-btn.normal-btn-main.btn-size-5 CREATE ACCOUNT

//- Block UnderConstruction -//
mixin countdown-block
    .countdown(data-start="1362139200" data-end="1388461320" data-now="1387461319" data-border-color="rgba(255, 255, 255, .8)")
        .clock-item.clock-days.countdown-time-value
            #canvas-days.clock-canvas
            .text
                p.val 0
                p.type-days.type-time Days
        .clock-item.clock-hours.countdown-time-value
            #canvas-hours.clock-canvas
            .text
                p.val 0
                p.type-hours.type-time Hours
        .clock-item.clock-minutes.countdown-time-value
            #canvas-minutes.clock-canvas
            .text
                p.val 0
                p.type-minutes.type-time Minutes
        .clock-item.clock-seconds.countdown-time-value
            #canvas-seconds.clock-canvas
            .text
                p.val 0
                p.type-seconds.type-time Seconds

