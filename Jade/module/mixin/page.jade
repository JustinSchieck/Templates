//- Block -//
mixin page-about-item(page_about)
    .page-about-item
        h4=page_about.title
        p!=page_about.content
        h5=page_about.author

//- Section -//
mixin page-about
    .page-about-header
        .container.text-center
            h5.color-gray= page_about_header.subtitle
            h2= page_about_header.title
    .page-about
        .container
            .row
                .col-md-4.col-sm-5.col-xs-12.quote
                    +page-about-item(data_page_about[0])
                .col-md-8.col-sm-7.col-xs-12.pragraph
                    +page-about-item(data_page_about[1])
//- Section No Header -//
mixin page-about-no-header
    .page-about
        .container
            .row
                .col-md-4.col-sm-5.col-xs-12.quote
                    +page-about-item(data_page_about[0])
                .col-md-8.col-sm-7.col-xs-12.pragraph
                    +page-about-item(data_page_about[1])

