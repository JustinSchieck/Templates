//- Block Shop 02 -//
mixin shop-product-2(shop)
    .shop-product-2
        .image
            .saleoff
                if shop.saleoff 
                    p=shop.saleoff
                else 
                    p(style="display:none;")
            img(src=shop.image alt="Shop Product")
            .overlay.text-center
                ul
                    li.clearfix
                        a(href="#" data-toggle="modal" data-target="#product-quick-view").quick-view: i.icon-search-icon
                        a(href="shop-cart.html").add-to-cart: i.icon-svg-icon-15
        .info
            a(href="shop-detail-2.html")=shop.content
            .price
                if shop.oldprice 
                    del=shop.oldprice
                else 
                    del(style="display:none;")
                span=shop.price
            if shop.rated
                +star-rating(shop.rated)
            else
                +star-rating(0)

