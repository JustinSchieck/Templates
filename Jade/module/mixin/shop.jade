//- Block Shop 01 -//
mixin shop-product-1(shop)
    .shop-product-1
        .image
            img(src=shop.image alt="Shop Product")
            .overlay
                ul
                    li
                        a(href="#" data-toggle="modal" data-target="#product-quick-view").quick-view: i.icon-search-icon
        .info.clearfix
            a.name(href="shop-detail-1.html")=shop.content
            a.add-to-cart(href="shop-cart.html"): i.icon-svg-icon-16
            .price
                if shop.oldprice 
                    del=shop.oldprice
                span=shop.price

