extends module/form
block page-config
    -page_option.body_class = null;
    -page_option.count_to = false;
    -page_option.count_down = false;
    -page_option.slider = true;
    -page_option.isotope = false;
    -page_option.slip_screen = false;
    -page_option.bootstrap_js = false;
    -page_option.jquery_ui = false;
    -page_option.magnific_popup = false;
block page-data
    - var data_about_header = {subtitle: "WHAT WE DO" , title: "WE ARE GOOD AT" , caption:"Wolverine Creative are an independent , one-stop creative specialist  with the experience & the know how to come up with great business solutions and make them happen"}
block before-main
    +main-nav-2-b1
block header
    header.site-header
    +common-static-header('HEADING')
block main
    section
        +section-header-1-1(data_about_header)
    section.bgc-gray
        +section-header-1-2(data_about_header)
    section
        +section-header-1-3(data_about_header)
    section.bgc-gray
        +section-header-2-1(data_about_header)
    section
        +section-header-2-2(data_about_header)
block footer
    +footer-preset-02
block after-main